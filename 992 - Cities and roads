#include <stdio.h>

int main(){
    short int n, count = 0;
    scanf("%hd", &n);
    short int arr[n][n];
    for (int i = 0; i < n; ++i){
        for (int j = 0; j < n; ++j){
            scanf("%hd", &arr[i][j]);
            if (arr[i][j] == 1) count++;
        }
    }
    printf("%hd", count / 2);

    return 0;
}
