#include <bits/stdc++.h>
#include <algorithm>
using namespace std;
int func(vector<int>& nums) {
    int n = nums.size(), Max = 1;
    vector<int> dp(n, 1);
    for (int i = 1; i < n; ++i){
        for (int j = 0; j < i; ++j){
            if (nums[i] > nums[j]){
                dp[i] = max(dp[i], dp[j] + 1);
                Max = max(Max, dp[i]);
            }
        }
    }
    return Max;
}
int main(){
    int n;
    cin >> n;
    vector<int> nums(n);
    for (int i = 0; i < n; ++i) cin >> nums[i]; 
    cout << func(nums);
    return 0;
}
